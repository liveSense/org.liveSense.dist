 <?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
  
  <shortcuts>
  
    <skipIfNotSupported />
  
    <programGroup defaultName="Remote Invoice" location="applications"/>
  
    <shortcut
       name="Start Server"
       programGroup="yes"
       desktop="yes"
       applications="no"
       startMenu="yes"
       startup="no"
       target="$INSTALL_PATH\bat\runConsole.bat"
       commandLine=""
       description="Start Remote Invoice"
       iconFile="%SystemRoot%\system32\SHELL32.dll"
       workingDirectory="$INSTALL_PATH">
       <createForPack name="Remote Invoice" />
       
     </shortcut>
  
     <shortcut
       name="Remote Invoice Uninstaller"
       programGroup="yes"
       desktop="no"
       applications="no"
       startMenu="yes"
       startup="no"
       target="$INSTALL_PATH\Uninstaller\uninstaller.jar"     
       commandLine=""
       iconFile="%SystemRoot%\system32\SHELL32.dll"
       iconIndex="31" 
       description="This uninstalls Remote Invoice">
       	<createForPack name="Remote Invoice" />
     </shortcut>
     
  </shortcuts>